<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
                              http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<configuration>
		<stop-on-errors>true</stop-on-errors>
		<date-format>yyyy-MM-dd</date-format>
		<wildcard>true</wildcard>
	</configuration>
	
	<mapping map-id="recognition" wildcard="true" >
		<class-a>com.cv.model.Recognition</class-a>
		<class-b>com.cv.vo.RecognitionVO</class-b>

		<field>
			<a>recId</a>
			<b>recId</b>
		</field>

		<field>
			<a>desc</a>
			<b>desc</b>
		</field>

		<field>
			<a>question</a>
			<b>question</b>
		</field>

		<field >
			<a>startDate</a>
			<b>startDate</b>
		</field>

		<field>
			<a>endDate</a>
			<b>endDate</b>
		</field>

	</mapping>

</mappings>
               